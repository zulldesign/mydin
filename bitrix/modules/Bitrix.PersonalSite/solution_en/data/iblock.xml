			<permission rolename="Guest">IBlockRead</permission>
		</permissions>
		<store item="iblock" optname="PhotosIBlockId" />
		<captions>
			<caption type="addelement">Add Photo</caption>
			<caption type="addsection">Add Album</caption>
			<caption type="changeelement">Edit Photo</caption>
			<caption type="changesection">Edit Album</caption>
