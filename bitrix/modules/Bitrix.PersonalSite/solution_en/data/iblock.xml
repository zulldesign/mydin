			<caption type="newelement">Photo</caption>
			<caption type="newsection">Album</caption>
			<caption type="sectionlist">List of Albums</caption>
			<caption type="sectionname">Album</caption>
			<caption type="sectionsname">Albums</caption>
		</captions>
	</iblock>
